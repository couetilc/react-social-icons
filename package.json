{
  "name": "react-social-icons",
  "version": "5.14.0",
  "description": "beautiful, easy svg social icons in react",
  "main": "./dist/react-social-icons.umd.js",
  "module": "./dist/react-social-icons.es.js",
  "types": "./dist/react-social-icons.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-social-icons.es.js",
      "require": "./dist/react-social-icons.umd.js"
    }
  },
  "scripts": {
    "build": "vite build -c vite.config.js",
    "build:watch": "npm run build -- --watch",
    "examples": "vite",
    "lint": "eslint src",
    "prepublish": "NODE_ENV=production npm run build",
    "start": "npm run examples",
    "test": "npm run lint && npm run test:js && npm run test:ts",
    "test:watch": "npm run test:js -- --watch",
    "test:ts": "npx tsc --jsx react --noEmit --strict --esModuleInterop test/SocialIcon.tsx",
    "test:js": "jest",
    "bundlesize": "npm run prepublish && tar -c build | gzip > build.tar.gz && du -h build build.tar.gz"
  },
  "author": "jaketrent",
  "license": "MIT",
  "contributors": [
    "Vitaly Aminev <v@makeomatic.ru>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaketrent/react-social-icons"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@vitejs/plugin-react": "^1.1.4",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/test-runner": "^0.13.27",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^27.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.67.0",
    "typescript": "^4.2.4",
    "vite": "^2.7.13"
  },
  "peerDependencies": {
    "react": "15.x.x || 16.x.x || 17.x.x || 18.x.x"
  },
  "jest": {
    "setupFilesAfterEnv": [],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0"
  }
}
